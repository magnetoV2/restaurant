    <!--Base-->
    <FontFamily x:Key="Base_Bold">>pack://application;,,,/Resources/Fonts/OpenSans/#Open Sans Bold</FontFamily>
    <FontFamily x:Key="Base_BoldItalic">pack://application;,,,/Resources/Fonts/OpenSans/#Open Sans Bold Italic</FontFamily>
    <FontFamily x:Key="Base_Regular">pack://application;,,,/Resources/Fonts/OpenSans/#Open Sans</FontFamily>
    <FontFamily x:Key="Base_RegularItalic">pack://application;,,,/Resources/Fonts/OpenSans/#Open Sans Italic</FontFamily>
    <FontFamily x:Key="Base_Light">pack://application;,,,/Resources/Fonts/OpenSans/#Open Sans Light</FontFamily>
    <FontFamily x:Key="Base_LightItalic">pack://application;,,,/Resources/Fonts/OpenSans/#Open Sans Light Italic</FontFamily>
